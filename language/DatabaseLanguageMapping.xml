<DatabaseLanguageMapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="xsd/DatabaseLanguageMapping.xsd" version="1">
   <!-- HMI Sprache zu DB Sprache Mapping, jeder HMI Sprache kann eine oder mehrere DB Sprachen zugeordnet werden -->
   <!-- Dabei kann zu jeder DB Sprache auch ein Fallback definiert werden -->
   <!-- Jeder HMI Sprache kann hier auch ein  Thesaurus Mapping Sprache zugeordnet werden, diese Thesaurussprache dient zur vervollstaendigung des Thesaurus Pfades -->
   <!-- Es duerfen NUR Sprachen definiert werden, zu denen auch eine POI-Sprach-Konfiguration (LangPoiConfig.xml) existiert!!!! -->
   <HmiLanguageMappingDefinitions>
      <!-- Default language definition must be presented! -->
      <LanguageDefinition id="default">
         <!-- Hmi language Id -->
         <HmiLanguageId>en_GB</HmiLanguageId>
         <!-- mapping to a db language/-s -->
         <DbLanguageIds>
            <DbLanguageId>en_GB</DbLanguageId>
         </DbLanguageIds>
         <!-- the thesaurus sub-folder -->
         <ThesaurusLanguageId>en_GB</ThesaurusLanguageId>
         <!-- a db lang code and its fall back. ATTENTION the fallback must exist !!-->
         <FallbackLanguageId>en_GB</FallbackLanguageId>
      </LanguageDefinition>
